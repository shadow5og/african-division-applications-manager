{
  "name": "african-division-applications-manager-nextjs",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "yarn cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "yarn generate:types && yarn cross-env NODE_OPTIONS=--no-deprecation PAYLOAD_SEED=true PAYLOAD_DROP_DATABASE=true next dev",
    "devsafe": "yarn generate:types && rm -rf .next && yarn cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "yarn cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "yarn cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "yarn cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "yarn cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "yarn cross-env NODE_OPTIONS=--no-deprecation next start",
    "prepare": "husky"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "beta",
    "@payloadcms/next": "beta",
    "@payloadcms/plugin-cloud": "beta",
    "@payloadcms/richtext-lexical": "beta",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "next": "15.0.0-canary.173",
    "payload": "beta",
    "react": "19.0.0-rc-3edc000d-20240926",
    "react-dom": "19.0.0-rc-3edc000d-20240926",
    "sharp": "0.32.6"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "eslint": "^8",
    "eslint-config-next": "15.0.0-canary.173",
    "husky": "^9.1.6",
    "typescript": "5.6.2"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-rc.1",
      "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
    }
  },
  "overrides": {
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
  }
}
